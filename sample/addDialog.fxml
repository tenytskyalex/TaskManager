<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="150.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="152.0" prefWidth="338.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="addBtn" layoutX="128.0" layoutY="110.0" mnemonicParsing="false" onAction="#secondAdd" text="Зберегти" />
            <TextField fx:id="new_task" layoutX="85.0" layoutY="14.0" prefHeight="25.0" prefWidth="169.0" promptText="Назва задачі" />
            <TextField fx:id="new_start" layoutX="23.0" layoutY="53.0" prefHeight="26.0" prefWidth="124.0" promptText="Початок" />
            <TextField fx:id="new_end" layoutX="192.0" layoutY="53.0" prefHeight="26.0" prefWidth="124.0" promptText="Кінець" />
      </children>
    </AnchorPane>
  </children>
</VBox>
